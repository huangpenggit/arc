			SELECT * FROM ( 					SELECT  					 COLUMN  					FROM 					tmc_flight_order tfo 					LEFT JOIN 					tmc_flight tf 					ON tfo.id = tf.orderid 					LEFT JOIN  					tmc_passenger tp 					ON  					tfo.id = tp.orderId 					LEFT JOIN  					t_org_info toi 					ON toi.orgCode = tfo.create_org_code  					LEFT JOIN  					tmc_supplier ts 					ON tfo.tmcSupplierId = ts.id 					WHERE  					tfo.productType = 'H' 					AND tf.ifRebookFlight = 0 					AND tfo.onBusiness = 1  					AND DATE_FORMAT( tfo.orderDate, '%Y%m' ) = DATE_FORMAT( ?, '%Y%m' )  					and tfo.orgCodePath like ?  					AND tfo.tmcSupplierId = ?  					AND tp.passengerName LIKE ?  					GROUP BY tp.employeeId 					) t  					WHERE 1=1 					AND t.priceRate BETWEEN ? AND ?;