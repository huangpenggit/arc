server {
    listen 443;
    ssl                  on;
    ssl_certificate      /etc/ssl/rm-cert.pem;
    ssl_certificate_key  /etc/ssl/rm-key.pem;
    ssl_verify_client on;
    ssl_client_certificate /etc/ssl/ca-cert.pem;
    ssl_protocols  SSLv2 SSLv3 TLSv1;
    ssl_ciphers  ALL:!ADH:!EXPORT56:RC4+RSA:+PKCS12:+HIGH:+MEDIUM:+LOW:+SSLv2:+EXP;
    ssl_prefer_server_ciphers   on;

    location /rmmm {
        rewrite /rmmm/?(.*\;)(jsessionid=\w*)(.*)$  /easy-mobile-mallshop/$1$2$3?$2 break;
        rewrite ^/rmmm/?(.*)$ /easy-mobile-mallshop/$1 break;
        proxy_pass        http://127.0.0.1:9340;
        proxy_cookie_path /easy-mobile-mallshop/ /rmmm/;
        proxy_redirect off;
        proxy_set_header Host $host;
        proxy_set_header X-Real-IP $remote_addr;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
    }

    location /app {
        rewrite /app/?(.*\;)(jsessionid=\w*)(.*)$  /easy-mobile-malllife/$1$2$3?$2 break;
